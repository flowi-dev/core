{
  "name": "@flowi-dev/core",
  "version": "0.1.1",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "test:coverage": "jest --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges",
    "test": "npm run test:badges",
    "lint": "eslint --cache --fix  .",
    "docs": "typedoc --plugin typedoc-plugin-markdown --out docs src/**/*",
    "prebuild": "rimraf lib && npm run lint && npm run test",
    "postbuild": "npm run docs",
    "build:compile": "babel src --out-dir lib --extensions \".ts,.tsx\" --ignore \"**/*.test.tsx\" --copy-files",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationMap --outDir lib --project tsconfig.json",
    "build": "concurrently npm:build:*",
    "bump:patch": "npm version patch",
    "bump:minor": "npm version minor",
    "bump:major": "npm version major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flowi-dev/core.git"
  },
  "author": "JanuZz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flowi-dev/core/issues"
  },
  "homepage": "https://github.com/flowi-dev/core#readme",
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-react": "^7.22.3",
    "@babel/preset-typescript": "^7.21.5",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "concurrently": "^8.1.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "pretty-quick": "^3.1.3",
    "rimraf": "^5.0.1",
    "semver": "^7.5.1",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^3.15.3",
    "typescript": "^5.1.3"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "dependencies": {
    "@types/react": "^18.2.8",
    "jest-coverage-badges": "^1.1.2",
    "react": "^18.2.0"
  },
  "pre-commit": [
    "bump:patch",
    "lint"
  ]
}
